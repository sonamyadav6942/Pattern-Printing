 Square Star Pattern
 
*****
*****
*****
*****
*****

// Usinhg for loop 
#include<iostream>
using namespace std;
int main()
{
	int i,j,n;
	cout << "Enter number of rows/column : ";
	cin >> n;
	for(i = 1; i<= n; i++)
	{
		for(j = 1; j<= n; j++)
		{
			cout << "*";
		}
		cout << endl;
	}
  return 0;
}

-----------------------------------------------------------------------------------------------------------------------------------------------------------------------

***
***
***
#include<iostream>
using namespace std;
int main()
{
	int i,j,n;
	cout << "Enter number of rows/column : ";
	cin >> n;
	i = 1;
	while(i <= n)
	{
		j = 1;
		while(j <= n)
		{
		cout << "*";
		j++;	
		}
		i++;
		cout << endl;
	}
	return 0;
}


